<aura:component controller="VehicleVersionController"  implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes" access="global" >
    
    <ltng:require styles="/resource/1465639409000/ASI_JS_Bootstrap_V3_3_6/ASI_JS_Bootstrap_V3_3_6/dist/css/bootstrap.min.css,
                          /resource/1465878109000/ASI_JS_JQuery_V1_9_1/css/jquery-ui.css,
                          /resource/1465873790000/noUiSlider/nouislider.min.css,
                          /resource/1466129618000/FontAwesomeCSS" 
                  scripts="/resource/1465786063000/modal/jquery-2.2.4.js,
                           /resource/1465878109000/ASI_JS_JQuery_V1_9_1/js/jquery-ui.min.js,
                           /resource/1465639409000/ASI_JS_Bootstrap_V3_3_6/ASI_JS_Bootstrap_V3_3_6/dist/js/bootstrap.js,
                           /resource/1465873790000/noUiSlider/nouislider.min.js"   
                  afterScriptsLoaded="{!c.doInit}"   />
    
    <ltng:require styles="/resource/1465970842000/slds/assets/styles/salesforce-lightning-design-system.css"/>
    <ltng:require styles="/resource/1465970842000/slds/assets/styles/salesforce-lightning-design-system-vf.css"/>
    <ltng:require styles="/resource/1465970842000/slds/assets/styles/salesforce-lightning-design-system-ltng.css" />
    
    
    <aura:attribute type="VehicleVersionController" name="counts" />
    <aura:attribute type="Object" name="displayItems" />
    
    <aura:attribute type="Object" name="VersionListMap" />
    <aura:attribute type="String[]" name="CompareProductIdList" />
    <aura:attribute type="Integer[]" name="PriceList" />
    <aura:attribute type="Integer" name="CurrentOrder" />
    
    <aura:attribute type="String" name="selectID" />
    
    <aura:attribute type="String" name="CompareTableHTML" />
    
     <aura:attribute name="leadlist" type="Lead[]" />
    
 <aura:attribute name="Emailleadlist" type="Lead[]" />
    
    <aura:attribute name="Displaybtn" type="Boolean" default="false" />
	
    <aura:attribute name="CreateNewUser" type="Boolean" default="false" />
    
    
    <aura:attribute name="VehicleOrder" type="Vehicle_Order__c"  default="{ 'sobjectType': 'Vehicle_Order__c',
                         'Order_Date__c': '16-06-2016',
                         'Order_Amount__c': 0 ,
                         'Invoice__c': '' }" />

    
    <aura:handler name="change" value="{!v.items}" action="{!c.itemsChange}"/>
    
    
    
    
    <div class="container">
        <div class="row" style= " margin-top: 1em">
            <br/>
            
        </div>
        <div class="row" style= " margin-top: 1em">
            <div class="col-md-2"></div>
            <div class="col-md-1"></div>
            <div class="col-md-5"  style=" text-align: center;" > 
                
                <div style="display:inline-block;padding-left: 1em; text-align: right; float: left;width:90%">
                    <input type="text" class="form-control" 
                           id="MainSearchPartId" 
                           onkeyup="{!c.MainSearchPart}"  onchange="{!c.MainSearchPart}" 
                           placeholder="Search Car"  style="width:110%"/> 
                </div>
                
                <div style="display:inline-block;text-align: left;float: right;width:10%">
                    <button type="button" class="btn btn-default" onclick="{!c.MainSearchPart}">
                        <span class="glyphicon glyphicon-search" aria-hidden="true"></span> 
                    </button>
                </div>
                
            </div>
            <div class="col-sm-2"></div>
        </div>
        
        <div class="row" style= " margin-top: 1em">
            <div class="col-md-2"></div>
            <div class="col-md-1"></div>
            <div class="col-md-5" style=" text-align: center;"> 
                
                <div class="row">
                    <div style=" display:inline-block;padding-right: 0.5em;padding-left: 0.5em;">
                        Amount From
                    </div>
                    <div style=" display:inline-block;">
                        <ui:inputSelect class="dynamic form-control SelectField " aura:id="InputMinAmount" change="{!c.ChangeAmountFilter}"   />
                    </div>
                    <div style=" display:inline-block;padding-right: 0.5em;padding-left: 0.5em;">
                        To
                    </div>
                    <div style=" display:inline-block;">
                        <ui:inputSelect class="dynamic form-control SelectField" aura:id="InputMaxAmount"  change="{!c.ChangeAmountFilter}"  />  
                    </div>
                </div>
                
                
            </div>
            <div class="col-md-2"></div>
            
        </div>
        
        
        
        
        <div class="border row" style=" margin-top: 1em">
            <aura:iteration var="dItems" items="{!v.displayItems}">
                <div class="border col-md-3"> 
                    <div class="panel panel-default" >
                        <div class="panel-body"  style=" text-align: center;" >
                            
                            
                            <a href="{!'https://elufa-sdfc-poc-dev-ed--c.ap2.visual.force.com/apex/Testing?scontrolCaching=1&amp;id='+ dItems.data.Vehicle_Model__c}">
                                <img src='{!dItems.data.Image__c}' alt="Car" style="width:14.8em;height:8em" />
                            </a>
                            <br/>
                            <div style=" text-align: center;font-size: 90%;" ><ui:outputText value="{!dItems.data.Name}" class="fontweight"/>  &nbsp;&nbsp;</div>
                            <div style=" text-align: center;font-size: 80%;"  ><ui:outputCurrency value="{!dItems.data.Price__c}"/></div>
                            
                            <div style=" display: inline-block; position: relative;"><span class="glyphicon glyphicon-calendar" style="display: inline-block; position: absolute; left: 0.50em; top: 30%;" aria-hidden="true"></span><input  class="btn btn-default" type="button" onclick="{!c.Pressbooking}" id="{!dItems.data.Id+'_book'}"  value="Testing" style="padding-left: 1.7em; width:5.8em">  </input></div>
                            <div style=" display: inline-block; position: relative; "><span class="glyphicon glyphicon-zoom-in" style="display: inline-block; position: absolute; left: 0.50em; top: 30%;" aria-hidden="true"></span><input  class="btn btn-default" type="button" onclick="{!c.PressCompare2}" id="{!dItems.data.Id+'_Com'}"  value="Compare" style="padding-left: 1.5em;">  </input></div>
                              
                            <br/>
                            <div style=" display: inline-block; position: relative; "><span class="glyphicon glyphicon-share" style="display: inline-block; position: absolute; left: 0.50em; top: 30%;" aria-hidden="true"></span><input  class="btn btn-default" type="button" onclick="{!c.PressShare}" id="{!dItems.data.Id+'_Share'}"  value="Share" style="padding-left: 2em;width:6.8m">  </input></div>
                            <div style=" display: inline-block; position: relative; "><span class="glyphicon glyphicon-shopping-cart" style="display: inline-block; position: absolute; left: 0.50em; top: 30%;" aria-hidden="true"></span><input  class="btn btn-default" type="button" onclick="{!c.PressOrder}" id="{!dItems.data.Id+'_order'}"  value="Order" style="padding-left: 2em; width:7.8m ">  </input></div>
                            
                            
                            
                        </div>
                    </div>
                </div>
            </aura:iteration>
        </div>
         
        <div class="row" >
            <div class="col-md-4"><button id="PreviousDiv" type="button" class="btn btn-default" onclick="{!c.PressPrevious}"   >Previous</button></div>
              <div class="col-md-4" id="PageDiv">Page 1/2 </div>
            <div class="col-md-4"><button id="NextDiv" type="button" class="btn btn-default" style="float: right;" onclick="{!c.PressNext}"   >Next</button></div>
        </div>
        
        
        <div class="row"  style=" margin-top: 1em" >
            <div id="SendEmailBTN" >
                <aura:renderIf isTrue="{!v.Displaybtn}">
