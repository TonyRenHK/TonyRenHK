                    <aura:set attribute="else">
                       
                        
                    </aura:set>
                </aura:renderIf>
            </div>
        </div>
        
        <div class="row" style= " margin-top: 1em">
            <div id="CompareTable"></div>
        </div>
        
        
        
          <div class="slds">
            
