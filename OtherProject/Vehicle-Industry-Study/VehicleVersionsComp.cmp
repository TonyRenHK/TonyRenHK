                                        <ui:inputNumber aura:id="OrderAmount" format="#,##0,000.00#"   class="form-control" />
                                    </div>
                                    
                                </div>
                                
                                
                                 <div class="slds-form-element__row" style="margin-top: 0.5em">
                                    <div class="slds-form-element2">
                                        <label class="slds-form-element__label" for="input-02">Contract Number(Optional)</label>
                                       <ui:inputPhone aura:id="ContractNumber" class="field form-control" />
                                    </div>
                                    
                                </div>
                                
                                <div class="slds-form-element__row" style="margin-top: 0.5em">
                                    <div class="slds-form-element ">
                                        <label class="slds-form-element__label" for="input-02">Select Account (Optional) </label>
                                         <ui:inputSelect class="dynamic form-control" aura:id="InputAccountDynamic" />
                                
                                    </div>
                                </div>
                             
                                
                            </div>
                            <div class="slds-col slds-size--1-of-6"></div>
                        </div>
                        
                        
                       
                    </div>
                    
                    <div class="slds-modal__footer" style="border-right-style: solid;border-bottom-style: solid;border-left-style: solid;border-width: medium;"  >
                        <div class="slds-x-small-buttons--horizontal">
                            <button class="slds-button slds-button--neutral" onclick="{!c.hideorderModal}">Cancel</button>
                            <button class="slds-button slds-button--brand" onclick="{!c.orderSave}">Save</button>
                        </div>
                    </div>
                </div>
                
            </div>
            
            <div class="slds-backdrop slds-backdrop--hide" aura:id="backdrop">
                
            </div>
            
        </div>
        
          
         
